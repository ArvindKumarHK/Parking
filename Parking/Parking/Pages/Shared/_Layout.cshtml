<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Parking</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Parking.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
            <div class="container">
                <a class="navbar-brand" asp-area="Pages" asp-page="/ParkingZone/Home/Index">
                    <img src="/Images/eye96.png" alt="ParkingZone Icon" class="mr-2" height="50" />
                </a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item">
                            <a class="nav-link" asp-area="Pages" asp-page="/ParkingZone/Home/Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="Pages" asp-page="/ParkingZone/DeviceDashboard/Index">Dashboard</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="Pages" asp-page="/ParkingZone/Devices/Index">Devices</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-page="/ParkingZone/Logs/Index">Logs</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="Pages" asp-page="/ParkingZone/Users/Index">Users</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="Pages" asp-page="/ParkingZone/Cameras/Index">Cameras</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="Pages" asp-page="/ParkingZone/CameraLogs/Index">CameraLogs</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="Pages" asp-page="/ParkingZone/Timeline/Index">Timeline</a>
                        </li>
@*                         <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Index">Home</a>
                        </li> *@
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-light bg-dark">
        <div class="container text-center">
            &copy; 2023 - <strong>Parking Zones</strong> - Parking Zones Management
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>